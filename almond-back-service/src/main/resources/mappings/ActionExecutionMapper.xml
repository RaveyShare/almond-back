<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ravey.almond.service.dao.mapper.ActionExecutionMapper">
    
    <select id="selectByAlmondId" resultType="com.ravey.almond.service.dao.entity.ActionExecution">
        SELECT 
            id,
            almond_id,
            actual_start,
            actual_end,
            create_time,
            update_time
        FROM action_execution
        WHERE almond_id = #{almondId}
        LIMIT 1
    </select>
    
</mapper>
